{
  "name": "@didi-js/didi-cli",
  "version": "0.0.1",
  "description": "CLI client for lib-didi. didi is a bundleless bundler for JavaScript and Typescript that transpiles common.js modules into distinct ESmodules.",
  "main": "index.js",
  "author": "Adam Crockett",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "commander-ts": "^0.1.5",
    "reflect-metadata": "^0.1.13",
    "@types/chalk": "^2.2.0",
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "nexe": "^3.3.3",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc -p ./src && node ./node_modules/nexe/index.js --input ./release/didi-cli-client/src/didi-cli.js --output ./release/didi && rm -rf ./release/didi-*"
  }
}
