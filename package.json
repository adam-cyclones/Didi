  "private": true,
  "license": "MIT",
  "dependencies": {
    "@types/koa": "^2.11.3",
    "@types/node": "^14.0.13",
    "@types/chalk": "^2.2.0",
    "chalk": "^4.1.0",
    "commander-ts": "^0.1.5",
    "console-log-tree": "^1.2.1",
    "koa": "^2.13.0",
    "mime-types": "^2.1.27",
